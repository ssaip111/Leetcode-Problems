//O(logn) â€“ Binary Search
public class Solution {
    public int FindMin(int[] nums) {
        
        int left = 0;
        int right = nums.Length-1;
        int mid =0;
        while(left < right)
        { 
            mid = (left+right)/2;
            if(nums[mid] > nums[right])
               left = mid +1;
            else  
               right = mid;
        }
       
       return nums[left];
    }
}

